<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="tolerance.request_identifier.monolog_processor" class="Tolerance\Bridge\Monolog\RequestIdentifier\RequestIdentifierProcessor">
            <argument type="service" id="tolerance.request_identifier.resolver" />

            <tag name="monolog.processor" />
        </service>
    </services>
</container>
